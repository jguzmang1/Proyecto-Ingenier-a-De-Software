        <servlet-name>CrearRutina</servlet-name>
        <url-pattern>/CrearRutina</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AdicionarRutina</servlet-name>
        <url-pattern>/AdicionarRutina</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ConsultarRutina</servlet-name>
        <url-pattern>/ConsultarRutina</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
